#pragma indent
using System.Console

def plus1(x)
  x + 1

def mul2(x)
  x * 2

// T1 -> T2 >> T2 -> void
$[1..3].Iter(plus1 >> WriteLine)

// T1 -> T2 >> T2 -> T3

$[1..3].Map(plus1 >> mul2).Iter(WriteLine)

def one()
  1

// void -> T1 >> T1 -> T2
def f = one >> plus1
WriteLine(f())

// void -> T1 >> T1 -> void

def g = one >> WriteLine
g()

/*
BEGIN-OUTPUT
2
3
4
4
6
8
2
1
END-OUTPUT
*/
